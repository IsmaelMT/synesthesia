{
  "name": "Synesthesia",
  "version": "0.0.1",
  "description": "Stereoscopic music viz",
  "license": "WTFPL",
  "scripts": {
    "start": "webpack --config config/webpack.dev.js | webpack-dev-server --port 8080 --config config/webpack.dev.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "build": "webpack --config config/webpack.prod.js --progress --profile --colors",
    "lint": "eslint ./src",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-register": "^6.3.13",
    "browserify": "^12.0.1",
    "clean-webpack-plugin": "^0.1.3",
    "copy-webpack-plugin": "^0.3.3",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^4.0.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.7.0",
    "jquery": "^2.2.3",
    "long": "^3.1.0",
    "minimist": "^1.2.0",
    "osc": "^2.0.3",
    "script-loader": "^0.7.0",
    "socket.io": "^1.4.6",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "@superguigui/wagner": "^0.1.13",
    "babel-preset-es2015": "^6.6.0",
    "core-js": "^2.0.0",
    "dat-gui": "^0.5.0",
    "glsl-checker": "^1.0.1",
    "glsl-noise": "0.0.0",
    "glslify": "^5.0.0",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.0.1",
    "lodash": "^3.10.1",
    "p5": "",
    "raw-loader": "^0.5.1",
    "three": "^0.75.0",
    "transform-loader": "^0.2.3"
  },
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "glslify": {
    "transform": [
      "glslify-fancy-imports",
      "glslify-hex"
    ]
  }
}
